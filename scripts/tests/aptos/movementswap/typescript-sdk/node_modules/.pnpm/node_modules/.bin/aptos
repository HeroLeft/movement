#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/aptos-cli/bin/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/aptos-cli/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/aptos-cli/bin/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/aptos-cli/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules/@aptos-labs/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/@aptos-labs+aptos-cli@0.1.8/node_modules:/Users/primata/Desktop/movement/scripts/tests/aptos/movementswap/typescript-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@aptos-labs/aptos-cli/bin/aptos" "$@"
else
  exec node  "$basedir/../@aptos-labs/aptos-cli/bin/aptos" "$@"
fi
